{
    ////////////////////////////////////////////////////////////////////////////
    // Configure your compose mixins here.
    // Comment out the ones you don't need.

    // Adds python debug
    "langchaintemplate.debug": "--file ./Compose/docker-compose-debug.yaml",

    // Adds GPU support
    "langchaintemplate.gpu": "--file ./Compose/docker-compose-nvidia.yaml",

    // Adds streamlit support
    "langchaintemplate.streamlit": "--file ./Compose/docker-compose-streamlit.yaml",
    
    // Mounts source code rather than copying it (e.g. live development)
    "langchaintemplate.mount": "--file ./Compose/docker-compose-mount.yaml",
    
    ////////////////////////////////////////////////////////////////////////////
    
    "python.autoComplete.extraPaths": [
        "Source/packages",
    ],
    "python.analysis.extraPaths": [
        "Source/packages"
    ],

    "cSpell.words": [
        "streamlit"
    ]
}
