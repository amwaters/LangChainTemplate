    "python.autoComplete.extraPaths": [
        "Source/packages",
    ],
    "python.analysis.extraPaths": [
        "Source/packages"
    ],

    "cSpell.words": [
        "chatbox",
        "selectbox",
        "streamlit"
    ]
}
