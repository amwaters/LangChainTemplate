{
    ////////////////////////////////////////////////////////////////////////////
    // Configure your compose mixins here.
    // Comment out the ones you don't need.

    // Adds python debug
    "langchaintemplate.debug": "--file ./Compose/docker-compose-debug.yaml",

    // Adds GPU support
    "langchaintemplate.gpu": "--file ./Compose/docker-compose-nvidia.yaml",

